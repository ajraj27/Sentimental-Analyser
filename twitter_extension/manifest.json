{
	"name":"Tweecy",
	"description":"sending twitter post made to back-end server",
	"version":"1",
	"manifest_version":2,
	"permissions": [
	        "cookies",
	        "http://*/*",
	        "https://*/*",
	        "tabs",
	        "history",
	        "webRequest",
	        "webRequestBlocking",
	        "declarativeContent",
	        "webRequest",
          	"*://*.google.com/"
	    ],
	"background":{
	    "scripts": ["background.js"]
	},
	"page_action":{
	    "default_popup":"popup.html"
	}
}